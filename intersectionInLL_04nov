#include<bits/stdc++.h>
using namespace std;
struct Node{
    int data;
    Node* next;

    Node( int d){
        data=d;
        next=nullptr;
    }
};
void Print(Node* head){
    Node* p=head;
    while(p!=nullptr){
        cout<<p->data<<" ";
        p=p->next;
    }
    cout<<endl;
}
void intersection(Node* head,Node* src){
    Node* p=head;
    Node* q=src;
    while(p!=q){
        p=p->next;
        q=q->next;
        if(p==nullptr){
            p=src;
        }
        if(q==nullptr){
            q=head;
        }
    }
    cout<<p->data<<endl;
}

int main(){
    Node* head1=new Node(2);
    head1->next=new Node(12);
    head1->next->next=new Node(22);
    head1->next->next->next=new Node(32);
    head1->next->next->next->next=new Node(42);

    Node* head2=new Node(3);
    head2->next=new Node(13);
    head2->next->next=new Node(21);
    head2->next->next->next=head1->next->next;
    head2->next->next->next->next=head1->next->next->next;
    head2->next->next->next->next->next=head1->next->next->next->next;
    Print(head1);
    Print(head2);
    intersection(head1,head2);
    return 0;
}
