#include<bits/stdc++.h>
using namespace std;
int main(){
    vector<int> vec={1,2,2,4,2,11,1,5,3,2};
    int n=vec.size();
    map<int,int>mp;
    /*for(int i=0;i<n;i++){
        mp[vec[i]]++;
    }
    int sum=0;
    for(auto &x:mp){
        int key=x.first;
        int value=x.second;
        sum+=key*value;
    }
    cout<<sum<<endl;*/

    for(int i=0;i<n;i++){
        if(mp.find(vec[i])==mp.end()) mp[vec[i]]=i;
    }
    for(auto &x:mp){
        int key=x.first;
        int value=x.second;
        cout<<key<<" occured at index "<<value<<endl;
    }

    return 0;
}
