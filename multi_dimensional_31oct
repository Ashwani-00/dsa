#include<bits/stdc++.h>
using namespace std;
int main(){
    int n=4;
    int arr[n][n];
    int x=n*n;
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            cin>>arr[i][j];
        }
    }
    int max=arr[0][0];
     for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            if(arr[i][j]>max){
                max=arr[i][j];
            }
        }
    }

    int a[max+1]={};
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            a[arr[i][j]]++;
        }
    }
    for(int i=0;i<max+1;i++){
        cout<<i<<": "<<a[i]<<endl;
    }
    return 0;
}

