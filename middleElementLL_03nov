#include <bits/stdc++.h>
using namespace std;
struct Node{
    int data;
    Node* next;
    Node(int v){
        data=v;
        next=NULL;
    }
};

void Print(Node* head){
    Node* p=head;
    int count=0;
    while(p!=nullptr){
        count++;
        p=p->next;
    }
    int mid=0;
    if(count%2==1){
        mid=(count+1)/2;
    }else{
        mid=count/2;
    }
    p=head;
    for(int i=1;i<mid;i++){
        p=p->next;
    }
    cout<<p->data<<endl;
}

void PrintLL(Node* head){
    Node* p=head;
    Node* q=head;
    while(p->next!=nullptr && p->next->next!=NULL){
        q=q->next;
        p=p->next->next;
    }
    cout<<q->data<<endl;
}

int main() {
    Node* head=new Node(12);
    head->next =new Node(22);
    head->next->next =new Node(32);
    head->next->next->next=new Node(42);
    head->next->next->next->next=new Node(52);
    head->next->next->next->next->next=new Node(62);

    PrintLL(head);
    Print(head);
    return 0;
}
