#include <bits/stdc++.h>
using namespace std;
struct Node{
    int data;
    Node* next;
    Node(int v){
        data=v;
        next=NULL;
    }
};
void Print(Node* head){
    Node* p=head;
    do{
        cout<<p->data<<" ";
        p=p->next;
    }while(p!=head);
    cout<<endl;
}
void insertinCircularLL(Node* head,int pos,int val){
    Node* p=head;
    Node* q=NULL;
    for(int i=1;i<pos;i++){
        q=p;
        p=p->next;
    }
    Node* newNode= new Node(val);
    newNode->next=p;
    q->next=newNode;
}
void insertinCircularLLhead(Node*& head,int val){
    Node* p=head;
    while(p->next!=head){
        p=p->next;
    }
    Node* newNode = new Node(val);
    p->next=newNode;
    newNode->next = head;
    head = newNode;
}
void insertinCircularLLtail(Node* head,int val){
    Node* p=head;
    while(p->next!=head){
        p=p->next;
    }
    Node* newNode= new Node(val);
    p->next=newNode;
    newNode->next=head;
}
int main() {
    Node* head=new Node(12);
    head->next =new Node(22);
    head->next->next =new Node(32);
    head->next->next->next =new Node(42);
    head->next->next->next->next=new Node(52);
    head->next->next->next->next->next=head;
    Print(head);
    int val=100;
    int pos=3;
    insertinCircularLL(head,pos,val);
    Print(head);
    int v1=35;
    insertinCircularLLhead(head,v1);
    Print(head);
    int v2=48;
    insertinCircularLLtail(head,v2);
    Print(head);
    return 0;
}
