#include<bits/stdc++.h>
using namespace std;
struct Node{
    int data;
    Node* next;

    Node( int d){
        data=d;
        next=nullptr;
    }
};

void Print(Node* head){
    Node* p=head;
    while(p!=nullptr){
        cout<<p->data<<" ";
        p=p->next;
    }
    cout<<endl;
}
void reverseLL(Node*& head){
    Node* p=head;
    Node* q=nullptr;
    Node* r=nullptr;
    while(p!=nullptr){
        r=q;
        q=p;
        p=p->next;
        q->next=r;
    }
    head=q;
}

int main(){
    Node* head=new Node(2);
    head->next=new Node(12);
    head->next->next=new Node(22);
    head->next->next->next=new Node(32);
    head->next->next->next->next=new Node(42);
    Print(head);
    reverseLL(head);
    Print(head);
    return 0;
}
